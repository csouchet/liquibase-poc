<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

	<property name="now" value="now()" dbms="mysql" />
	<property name="now" value="sysdate" dbms="oracle" />
	<property name="now" value="SYSDATE" dbms="postgresql" />

	<changeSet author="csouchet" id="1">
		<createTable tableName="ID_GENERATOR">
			<column name="NAME" type="varchar(255)" />
			<column name="VALUE" type="bigint" />
		</createTable>

		<insert tableName="ID_GENERATOR">
			<column name="NAME" value="ANIMAL" />
			<column name="VALUE" valueNumeric="30" />
		</insert>

		<insert tableName="ID_GENERATOR">
			<column name="NAME" value="OWNER" />
			<column name="VALUE" valueNumeric="0" />
		</insert>
	</changeSet>
</databaseChangeLog>